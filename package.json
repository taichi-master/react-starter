      },
      {
        "name": "Feedback",
        "url": "/feedback"
      },
      {
        "name": "Contact us",
        "url": "/contact"
      }
    ],
    "initialState": {
      "year": 2022,
      "counter": {
        "value": 0
      }
    }
  },
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js ./src ./server",
    "test": "jest",
    "dev": "NODE_ENV=development PORT=3000 nodemon -w server server",
    "preview": "webpack --env preview --stats-error-details",
    "build": "webpack --env build",
    "sass": "sass src/styles/main.scss:dist/main.css --watch --no-source-map",
    "start": "node server"
  },
  "jest": {
    "moduleNameMapper": {
      "^models/(.+)": "<rootDir>/src/models/$1",
      "^constants/(.+)$": "<rootDir>/src/models/constants/$1",
      "^actions": "<rootDir>/src/models/actions",
      "^actions/(.*)": "<rootDir>/src/models/actions/$1",
      "^reducers/(.+)": "<rootDir>/src/models/reducers/$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taichi-master/react-starter.git"
  },
  "keywords": [
    "isomorphic",
    "react",
