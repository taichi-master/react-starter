{
  "name": "react-starter",
  "version": "5.0.1",
  "description": "React Boilerplate",
  "main": "index.js",
  "cfg": {
    "name": "React Starter",
    "menuItems": [
      {
        "name": "Home",
        "url": "/"
      },
      {
        "name": "Hooks",
        "url": "/hooks"
      },
      {
        "name": "Lifecycle",
        "url": "/lifecycle"
      },
      {
        "name": "About",
        "url": "/about"
      }
    ],
    "footLinks": [
      {
        "name": "myApp v5.0.1",
        "url": "/about"
      },
      {
        "name": "Disclaimer",
        "url": "/disclaimer"
      },
      {
        "name": "Support",
